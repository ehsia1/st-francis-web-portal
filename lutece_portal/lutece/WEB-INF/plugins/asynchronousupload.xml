<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- Plugin Informations -->
    <name>asynchronous-upload</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.5</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>asynchronousupload.plugin.description</description>
    <provider>asynchronousupload.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/asynchronousupload/asynchronousupload.png</icon-url>
    <copyright>Copyright 2001-2014 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>4.4.1</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <css-stylesheets>
        <css-stylesheets-scope>portal</css-stylesheets-scope>
        <css-stylesheet>asynchronousupload/jquery.fileupload.css</css-stylesheet>
        <css-stylesheet>asynchronousupload/jquery.fileupload-ui.css</css-stylesheet>
    </css-stylesheets>
    
	<javascript-files>
        <javascript-files-scope>portal</javascript-files-scope>
        <javascript-file>asynchronousupload/vendor/jquery.ui.widget.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.iframe-transport.js</javascript-file>
         <javascript-file>asynchronousupload/load-image.min.js</javascript-file> 
        <javascript-file>asynchronousupload/jquery.fileupload.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.fileupload-process.js</javascript-file>
        <javascript-file>asynchronousupload/jquery.fileupload-image.js</javascript-file>
        <javascript-file>asynchronousupload/canvas-to-blob.min.js</javascript-file>
<!--         <javascript-file>asynchronousupload/jquery.fileupload-audio.js</javascript-file> -->
<!--         <javascript-file>asynchronousupload/jquery.fileupload-video.js</javascript-file> -->
        <javascript-file>asynchronousupload/jquery.fileupload-validate.js</javascript-file>
    </javascript-files>
</plug-in>